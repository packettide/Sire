{
    "name": "packettide/sire",
    "description": "Better Laravel 4 Generators",
    "keywords": ["laravel", "generators", "artisan", "bree"],
    "authors": [
        {
            "name": "Max Walker",
            "email": "max@packettide.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "mustache/mustache": "dev-master",
        "symfony/yaml": "2.*",
        "symfony/finder": "2.*",
        "packettide/bree": "1.*"
    },
    "require-dev": {
        "mockery/mockery": "dev-master",
        "illuminate/support": "~4",
        "illuminate/console": "~4",
        "illuminate/filesystem": "~4"
    },
    "autoload": {
        "psr-0": {
            "Packettide\\Sire": "src/"
        }
    },
     "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev"
}
